# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Todo {
  id: ID!
  text: String!
  done: Boolean!
  user: User!
}

type User {
  id: ID!
  name: String!
}


type Pokemon {
  id: ID!
  name: String!
  height: Int
  weight: Int
}

type Query {
  pokemon(id: ID!): Pokemon
  pokemons(limit: Int, offset: Int): [Pokemon!]!
}

input NewTodo {
  text: String!
  userId: String!
}

type Mutation {
  createTodo(input: NewTodo!): Todo!
}
